
<% users.each do |user| %>
  <% if is_not_requested?(user) %>
    <%= form_for @current_user,:url => add_friend_path(:friend_id => user.id) do |f| %>
      <tr>
        <td><%= user.id %> </td>
        <td><%= user.first_name + " " + user.last_name.to_s %></td>
        <td><%= user.date_of_birth %></td>
        <td><%= user.email %></td>
        <td>
          <div class="actions">
            <%= f.submit value="Add Friend" %>
          </div>
        </td>
      </tr>
    <% end %>
  <% end %>
<% end %>
