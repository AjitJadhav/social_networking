<p id="notice"> <%= notice %> </p>
<h2>People you may know.</h2>

<table>
  <tr>
    <th>id  </th>
    <th>Name</th>
    <th>Date of birth</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <%= form_for @current_user,:url => add_friend_path(:friend_id => user.id) do |f| %>
      <tr>
        <td><%= user.id %> </td>
        <td><%= user.name %></td>
        <td><%= user.date_of_birth %></td>
        <td><%= user.email %></td>
        <td>
          <div class="actions">
            <%= f.submit value="Add Friend" %>
          </div>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>

<br />


